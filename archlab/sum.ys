# Sample linked list

.pos 0
	irmovq stack, %rsp
	call main
	halt

.align 8
list:
ele1:
	.quad 0x00a
	.quad ele2
ele2:
	.quad 0x0b0
	.quad ele3
ele3:
	.quad 0xc00
	.quad 0
	
	
main:
	irmovq list, %rdi
	call sum_list
	ret


sum_list:
	subq %rax, %rax
	andq %rdi, %rdi
	je done
	
loop:
	mrmovq (%rdi), %rbx
	addq %rbx, %rax
	mrmovq 8(%rdi), %rdi
	andq %rdi, %rdi
	jne loop
done:	
	ret


.pos 0x200
stack:
